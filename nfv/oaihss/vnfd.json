{
    "name": "oaihss",
    "vendor": "Evolved Core Network",
    "version": "1",
    "lifecycle_event": [
        {
            "event": "CONFIGURE",
            "lifecycle_events": [
                "oaimme_relation_joined.sh"
            ]
        },
        {
            "event": "INSTANTIATE",
            "lifecycle_events": [
                "oaihss_install.sh"
            ]
        },
        {
            "event": "START",
            "lifecycle_events": [
                "oaihss_start.sh"
            ]
        }
    ],
    "configurations": {
        "name": "client-configuration",
        "configurationParameters": [
            {
                "confKey": "SCRIPTS_PATH",
                "value": "/opt/openbaton/scripts"
            }
        ]
    },
    "vdu": [
        {
            "vm_image": [],
            "scale_in_out": 1,
            "vnfc": [
                {
                    "connection_point": [
                        {
                            "virtual_link_reference": "mgmt_oa",
                            "interfaceId": 0
                        },
                        {
                            "virtual_link_reference": "net_d_oa",
                            "interfaceId": 1
                        }
                    ]
                }
            ],
            "vimInstanceName": [
            ]
        }
    ],
    "virtual_link": [
        {
            "name": "mgmt_oa"
        },
        {
            "name": "net_d_oa"
        }
    ],
    "deployment_flavour": [
        {
            "flavour_key": "m1.medium"
        }
    ],
    "type": "oaihss",
    "endpoint": "generic",
    "requires": {
        "oaimme": {
            "parameters": [
                "mgmt_oa_floatingIp",
                "hostname"
            ]
        }
    }
}
