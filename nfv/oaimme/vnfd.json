    "virtual_link": [
        {
            "name": "mgmt_oa"
        }
    ],
    "deployment_flavour": [
        {
            "flavour_key": "m1.medium"
        }
    ],
    "type": "oaimme",
    "endpoint": "generic",
    "requires": {
        "oaihss_theirs": {
            "parameters": [
                "mgmt_oa"
            ]
        },
        "oaispgw_theirs": {
            "parameters": [
                "mgmt_oa"
            ]
        }
    }
