{
    "name": "oaispgw",
    "vendor": "Evolved Core Network",
    "version": "1",
    "lifecycle_event": [
        {
            "event": "INSTANTIATE",
            "lifecycle_events": [
                "oaispgw_install.sh"
            ]
        },
        {
            "event": "CONFIGURE",
            "lifecycle_events": [
                "epcclient_alice_relation_joined.sh"
            ]
        },
        {
            "event": "START",
            "lifecycle_events": [
                "oaispgw_start.sh"
            ]
        }
    ],
    "configurations": {
        "name": "client-configuration",
        "configurationParameters": [
            {
                "confKey": "SCRIPTS_PATH",
                "value": "/opt/openbaton/scripts"
            },
            {
                "confKey": "UE_CIDR",
                "value": "192.168.3.0/24"
            },
            {
                "confKey": "UE_MTU",
                "value": "1500"
            }
        ]
    },
    "vdu": [
        {
            "vm_image": [],
            "scale_in_out": 1,
            "vnfc": [
                {
                    "connection_point": [
                        {
                            "virtual_link_reference": "mgmt_oa",
                            "interfaceId": 0
                        }
                    ]
                }
            ],
            "vimInstanceName": [
            ]
        }
    ],
    "virtual_link": [
        {
            "name": "mgmt_oa"
        }
    ],
    "deployment_flavour": [
        {
            "flavour_key": "m1.medium"
        }
    ],
    "type": "oaispgw",
    "endpoint": "generic"
}
